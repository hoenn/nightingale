<h1>Show Account</h1>

<ul>

  <li>
    <strong>Starting balance:</strong>
    <%= @account.starting_balance %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @account.name %>
  </li>

  <li>
    <strong>Owner:</strong>
    <%= @account.user_id %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.account_path(@conn, :edit, @account) %></span>
<span><%= link "Back", to: Routes.account_path(@conn, :index) %></span>

