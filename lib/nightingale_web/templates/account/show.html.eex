<h1>Show Account</h1>

<ul>

  <li>
    <strong>Starting balance:</strong>
    <%= @account.starting_balance %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @account.name %>
  </li>

  <li>
    <strong>Owner:</strong>
    <%= @account.owner %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_account_path(@conn, :edit, current_user_id(@conn), @account) %></span>
<span><%= link "Back", to: Routes.user_account_path(@conn, :index, current_user_id(@conn)) %></span>
